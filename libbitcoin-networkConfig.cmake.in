# - Config file for the LIBBITCOIN_NETWORK package
#
#  LIBBITCOIN_NETWORK_INCLUDE_DIRS - include directories for LIBBITCOIN_NETWORK
#  LIBBITCOIN_NETWORK_LIBRARIES    - libraries to link against

get_filename_component(LIBBITCOIN_NETWORK_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(LIBBITCOIN_NETWORK_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

include("${LIBBITCOIN_NETWORK_CMAKE_DIR}/libbitcoin-networkTargets.cmake")
set(LIBBITCOIN_NETWORK_LIBRARIES libbitcoin-network)

find_package(libbitcoin 3.0.0 REQUIRED)
